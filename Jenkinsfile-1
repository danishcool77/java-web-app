pipeline {
  agent { label 'DANISH_MACHINE' }
  options {
    buildDiscarder(logRotator(numToKeepStr: '5'))
  }
  
  environment
  {
  
  NAME = "danish"
  }
  stages {
  

			
    stage('Scan') {
	
			options
			{
			  timeout(time: 1, unit: 'HOURS')
			}
      steps {
       echo 'Hello ${NAME}'
       echo "workspace ${WORKSPACE}"
	        }
      }
    }
    
        }
