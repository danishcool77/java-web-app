pipeline {
  agent { label 'DANISH_MACHINE' }
  options {
    buildDiscarder(logRotator(numToKeepStr: '5'))
  }
  stages {
  

			
    stage('Scan') {
	
			options
			{
			  timeout(time: 1, unit: 'HOURS')
			}
      steps {
        cmd = "java -version"
	bat cmd
        }
      }
    }
    
        }
