pipeline {
  agent { label 'DANISH_MACHINE' }
  options {
    buildDiscarder(logRotator(numToKeepStr: '5'))
  }
  
 
  stages {
  

			
    stage('Scan1') {
    
     environment
  {
  
  NAME = "danish"
   AAA_SECRET_TEXT = credentials('GIT_REPO_')
  }
	
		      steps {
       echo "Hello ${NAME}"
       echo "credentails  ${AAA_SECRET_TEXT}"
       echo "Branch ${env.GIT_BRANCH}"
	        }
      }
      
          stage('Scan2') {
	
		      steps {
       echo "Hello ${NAME}"
       echo "workspace ${WORKSPACE}"
       echo "Branch ${env.GIT_BRANCH}"
	        }
      }
      
    }
    
        }
